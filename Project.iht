from odoo import models, fields, api
import json

class Projectiht(models.Model):
  _inherit = "project.project"

  lokasi_proyek = fields.Text( string = "Lokasi Proyek", help = "input tempat proyek")

  source_aplikasi_pendukung = fields.Char( string = "url", help = "input link url")
  
  daftar_perkerja_remote = fields.Json( string="Daftar Pekerja Remote", help = :"input data pekerja")
